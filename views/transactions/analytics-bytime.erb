
<h3>See all transactions by time. The most recent transaction appears first.</h3>

<table>
  <tr>
    <th>Merchant logo</th>
    <th>Merchant Name</th>
    <th>Amount</th>
    <th>Time</th>
    <th>Category</th>
    <th>Category logo</th>

  </tr>
  <% for item in @transactions_by_time %>
  <tr>
    <td><img src="<%= item['m_logo'] %>" alt="Merchant Logo"></td>
    <td><%= item['merchant_name'] %></td>
    <td><%= item['amount'] %></td>
    <td><%= item['time'] %></td>
    <td><%= item['label'] %></td>
    <td><img src="<%= item['t_logo'] %>" alt="Tag Logo"></td>
  </tr>
  <% end %>
</table>
<br><br>
